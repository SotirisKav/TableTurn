/* ===== MODERN REGISTRATION FORM STYLES ===== */

.registration-container {
    min-height: 100vh;
    background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
    display: flex;
    align-items: flex-start;
    justify-content: stretch;
    padding: 0;
    width: 100vw;
    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, sans-serif;
}

.registration-card {
    background: white;
    border-radius: 0;
    box-shadow: 0 25px 50px rgba(0, 0, 0, 0.15);
    width: 100vw;
    margin: 0;
    min-width: 0;
    max-width: none;
    overflow: hidden;
    animation: slideUpNew 0.6s ease-out;
    display: flex;
    flex-direction: column;
    align-items: stretch;
}

@keyframes slideUpNew {
    from {
        opacity: 0;
        transform: translateY(30px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

.registration-header {
    background: linear-gradient(135deg, var(--aegean-blue) 0%, #1e3a8a 100%);
    color: white;
    padding: 40px;
    text-align: center;
}

.registration-header h1 {
    margin: 0 0 10px 0;
    font-size: 2.5rem;
    font-weight: 700;
    letter-spacing: -0.02em;
}

.registration-header p {
    margin: 0;
    font-size: 1.1rem;
    opacity: 0.9;
    font-weight: 300;
}

/* Progress Steps */
.progress-steps {
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 30px 40px;
    background: #f8fafc;
    border-bottom: 1px solid #e2e8f0;
}

.progress-step {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 8px;
    color: #64748b;
    transition: all 0.3s ease;
}

.progress-step.active {
    color: var(--aegean-blue);
}

.step-number {
    width: 40px;
    height: 40px;
    border-radius: 50%;
    border: 2px solid #e2e8f0;
    display: flex;
    align-items: center;
    justify-content: center;
    font-weight: 600;
    font-size: 1.1rem;
    background: white;
    transition: all 0.3s ease;
}

.progress-step.active .step-number {
    background: var(--aegean-blue);
    border-color: var(--aegean-blue);
    color: white;
    transform: scale(1.1);
}

.progress-step span {
    font-size: 0.9rem;
    font-weight: 500;
}

.progress-line {
    height: 2px;
    width: 100px;
    background: #e2e8f0;
    margin: 0 20px;
    transition: all 0.3s ease;
}

.progress-line.active {
    background: var(--aegean-blue);
}

/* Form Sections */
.registration-form {
    padding: 0;
}

.form-section {
    padding: 40px;
}

.section-header {
    text-align: center;
    margin-bottom: 40px;
}

.section-header h2 {
    color: #1e293b;
    font-size: 1.8rem;
    font-weight: 600;
    margin: 0 0 8px 0;
}

.section-header p {
    color: #64748b;
    font-size: 1rem;
    margin: 0;
}

/* Form Fields */
.form-field {
    margin-bottom: 24px;
}

.form-field label {
    display: block;
    margin-bottom: 8px;
    color: #374151;
    font-weight: 500;
    font-size: 0.95rem;
}

.form-field input,
.form-field textarea,
.form-field select {
    width: 100%;
    padding: 16px 20px;
    border: 2px solid #e5e7eb;
