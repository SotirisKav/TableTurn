  justify-content: center;
  align-items: flex-start; /* Changed from center */
}

.setup-container {
  max-width: 1100px; /* Increased from 900px */
  width: 100%;
  margin: 0 auto;
  padding: 0 2rem;
}

.setup-header {
  text-align: center;
  margin-bottom: 2rem;
  background: var(--white);
  padding: 2rem;
  border-radius: 16px 16px 0 0;
  box-shadow: 0 2px 10px rgba(30, 58, 138, 0.1);
}

.setup-header h1 {
  font-size: 2.5rem;
  font-weight: 700;
  color: var(--aegean-blue);
  margin-bottom: 0.5rem;
}

.setup-header p {
  color: var(--text-gray);
  font-size: 1.2rem;
  margin-bottom: 2rem;
}

.setup-progress {
  max-width: 400px;
  margin: 0 auto;
}

.progress-bar {
  width: 100%;
  height: 8px;
  background: #e0e7ef;
  border-radius: 4px;
  overflow: hidden;
  margin-bottom: 0.5rem;
}

.progress-fill {
  height: 100%;
  background: linear-gradient(90deg, var(--aegean-blue), var(--deep-blue));
  transition: width 0.5s ease;
  border-radius: 4px;
}

.progress-text {
  font-size: 0.9rem;
  color: var(--text-gray);
  font-weight: 600;
}

.setup-chat-container {
  background: var(--white);
  border-radius: 0 0 16px 16px;
  box-shadow: 0 4px 20px var(--shadow);
  display: flex;
  flex-direction: column;
  overflow: hidden; /* Keep this to prevent outer scroll */
}

.chat-messages {
  flex: 1;
  padding: 2rem;
  overflow-y: auto;
  display: flex;
  flex-direction: column;
  gap: 1.5rem;
  background: #fafbfc;
}

/* Fix Message Styling */
.message {
  display: flex;
  max-width: 85%;
  margin-bottom: 1rem;
}

.message.user {
  align-self: flex-end;
  justify-content: flex-end;
}

.message.ai {
  align-self: flex-start;
  justify-content: flex-start;
}

.message-content {
  padding: 1.2rem 1.5rem;
  border-radius: 20px;
  position: relative;
  max-width: 100%;
  word-wrap: break-word;
  box-shadow: 0 2px 12px rgba(30, 58, 138, 0.08);
}

.message.user .message-content {
  background: linear-gradient(135deg, var(--aegean-blue) 0%, var(--deep-blue) 100%);
  color: var(--white);
  border-bottom-right-radius: 8px;
}

.message.ai .message-content {
  max-width: 100%; /* Full width for map messages */
  width: 100%;
}

.message-text {
  line-height: 1.6;
  margin-bottom: 0.5rem;
  font-size: 1.05rem;
  word-wrap: break-word;
  overflow-wrap: break-word;
}

.message-time {
  font-size: 0.75rem;
  opacity: 0.7;
  text-align: right;
  margin-top: 0.5rem;
}

/* Map Container Specific Styling */
.map-container {
  margin: 1.5rem 0;
  border-radius: 12px;
  overflow: hidden;
  box-shadow: 0 4px 16px rgba(30, 58, 138, 0.15);
  border: 2px solid var(--soft-blue);
  width: 100%;
  height: 450px; /* Increased height */
}

/* Typing Indicator */
.typing-indicator {
  display: flex;
  gap: 6px;
  align-items: center;
  padding: 1rem 0;
  justify-content: center;
}

.typing-dot {
  width: 8px;
  height: 8px;
  border-radius: 50%;
  background: var(--aegean-blue);
  animation: typing 1.4s infinite ease-in-out;
}

.typing-dot:nth-child(1) { animation-delay: -0.32s; }
.typing-dot:nth-child(2) { animation-delay: -0.16s; }
.typing-dot:nth-child(3) { animation-delay: 0s; }

@keyframes typing {
  0%, 80%, 100% { 
    transform: scale(0.8);
    opacity: 0.5;
  }
  40% { 
    transform: scale(1.2);
    opacity: 1;
  }
}

/* Input Section */
.chat-input-section {
  border-top: 1px solid #e0e7ef;
  padding: 1.5rem 2rem;
  background: var(--white);
}

.chat-input-container {
  display: flex;
  gap: 1rem;
  align-items: flex-end;
}

.chat-input {
  flex: 1;
  padding: 1rem 1.2rem;
  border: 1.5px solid #e0e7ef;
  border-radius: 16px;
  outline: none;
  resize: none;
  font-family: inherit;
  font-size: 1rem;
  background: #f8fafc;
  transition: all 0.2s ease;
  line-height: 1.4;
}

.chat-input:focus {
  border-color: var(--aegean-blue);
  background: var(--white);
  box-shadow: 0 2px 8px rgba(30, 58, 138, 0.1);
}

.send-button {
  padding: 1rem 1.5rem;
  background: var(--aegean-blue);
  color: var(--white);
  border: none;
  border-radius: 16px;
  cursor: pointer;
  display: flex;
  align-items: center;
  gap: 0.5rem;
  font-weight: 600;
  font-size: 1rem;
  transition: all 0.3s ease;
  box-shadow: 0 2px 12px rgba(30, 58, 138, 0.2);
}

.send-button:hover:not(:disabled) {
  background: var(--deep-blue);
  transform: translateY(-2px);
  box-shadow: 0 4px 16px rgba(30, 58, 138, 0.3);
}

.send-button:disabled {
  opacity: 0.5;
  cursor: not-allowed;
  transform: none;
  box-shadow: none;
}

/* Mobile Responsive */
@media (max-width: 768px) {
  .restaurant-setup-page {
    padding: 100px 1rem 1rem 1rem;
  }
  
  .setup-container {
    padding: 0;
  }
  
  .setup-chat-container {
    height: 500px;
  }
  
  .chat-messages {
    padding: 1rem;
    gap: 1rem;
  }
  
  .message {
    max-width: 95%;
  }
  
  .message-content {
    padding: 1rem 1.2rem;
  }
  
  .chat-input-section {
    padding: 1rem;
  }
  
  .chat-input-container {
    flex-direction: column;
    gap: 1rem;
  }
  
  .send-button {
    align-self: stretch;
    justify-content: center;
  }
