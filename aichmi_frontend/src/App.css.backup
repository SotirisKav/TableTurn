    border-top: 1px solid #e2e8f0;
    display: flex;
    gap: 1rem;
    justify-content: center;
}

.primary-button {
    background: #059669;
    color: white;
    border: none;
    border-radius: 0.5rem;
    padding: 0.75rem 2rem;
    cursor: pointer;
    font-weight: 500;
    transition: background 0.2s;
}

.primary-button:hover {
    background: #047857;
}

.secondary-button {
    background: #6b7280;
    color: white;
    border: none;
    border-radius: 0.5rem;
    padding: 0.75rem 2rem;
