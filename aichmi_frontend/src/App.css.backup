/* Opening Hours & Menu Items Form Styling */

/* Section styling consistent with restaurant info */
.section {
    margin-bottom: 3rem;
}

.section-title {
    font-size: 1.4rem;
    font-weight: 600;
    color: #1e293b;
    margin-bottom: 0.5rem;
    text-align: left;
}

.section-subtitle {
    color: #64748b;
    font-size: 0.95rem;
    margin-bottom: 1.5rem;
    text-align: left;
    line-height: 1.5;
}

/* Opening hours styling - enhanced and consistent */
.hours-grid {
    display: grid;
    gap: 12px;
    background: #fafafa;
    padding: 24px;
    border-radius: 16px;
    border: 2px solid #e5e7eb;
}

.day-row {
    display: flex;
    align-items: center;
