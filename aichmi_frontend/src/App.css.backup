/* Greek-inspired minimalist design for Aichmi */
:root {
  --aegean-blue: #1e3a8a;
  --deep-blue: #1e40af;
  --soft-blue: #e0f2fe;
  --white: #ffffff;
  --cream: #fefefe;
  --text-dark: #1f2937;
  --text-gray: #6b7280;
  --shadow: rgba(30, 58, 138, 0.1);
  --transition: all 0.3s ease;
  
  /* Modern design system */
  --gradient-primary: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
  --gradient-accent: linear-gradient(135deg, #f093fb 0%, #f5576c 100%);
  --gradient-hero: linear-gradient(135deg, #667eea 0%, #764ba2 50%, #f093fb 100%);
  --glass-bg: rgba(255, 255, 255, 0.1);
  --glass-border: rgba(255, 255, 255, 0.2);
  --card-shadow: 0 8px 32px rgba(31, 38, 135, 0.37);
  --hover-shadow: 0 15px 35px rgba(31, 38, 135, 0.2);
  --border-radius: 16px;
  --border-radius-large: 24px;
}

* {
  box-sizing: border-box;
  margin: 0;
  padding: 0;
}

body {
  font-family: 'Inter', -apple-system, BlinkMacSystemFont, sans-serif;
  line-height: 1.6;
  color: var(--text-dark);
  background: var(--white);
  width: 100%;
  overflow-x: hidden;
}

/* Header */
.header {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  background: var(--white);
  border-bottom: 3px solid var(--aegean-blue);
  z-index: 1000;
  box-shadow: 0 2px 10px rgba(30, 58, 138, 0.1);
}

.nav-container {
  max-width: 1200px;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 1rem 2rem;
}

.logo {
  font-size: 1.75rem;
  font-weight: 700;
  color: var(--aegean-blue);
  text-decoration: none;
  letter-spacing: 0.5px;
}

.nav-links {
  display: flex;
  list-style: none;
  gap: 2rem;
}

.nav-links a {
  color: var(--text-dark);
  text-decoration: none;
  font-weight: 700;
  transition: var(--transition);
  position: relative;
}

.nav-links a::after {
  content: '';
  position: absolute;
  bottom: -5px;
  left: 0;
  width: 0;
  height: 2px;
  background: var(--aegean-blue);
  transition: var(--transition);
}

.nav-links a:hover::after {
  width: 100%;
}

.nav-links a:hover {
  color: var(--aegean-blue);
}

.mobile-menu-toggle {
  display: none;
  flex-direction: column;
  background: none;
  border: none;
  cursor: pointer;
  padding: 0.5rem;
}

.mobile-menu-toggle span {
  width: 25px;
  height: 2px;
  background: var(--text-dark);
  margin: 3px 0;
  transition: var(--transition);
}

/* Hero Section */
.hero {
  position: relative;
  width: 100vw;
  height: calc(100vh - 80px);
  min-height: 600px;
  margin-left: calc(50% - 50vw);
}

.no-hero-gap {
  margin-top: 0 !important;
}

.hero-image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  margin: 0;
  padding: 0;
}

.hero-image img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  object-position: center;
}

.hero-overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: none;
  z-index: 2;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0;
  padding: 0;
}

.hero-content {
  text-align: center;
  color: white;
  max-width: 600px;
  padding: 0 2rem;
  animation: fadeInUp 1s ease-out;
  transition: color 0.8s ease-in-out;
}

.hero-title {
  font-size: clamp(2.5rem, 5vw, 4rem);
  font-weight: 700;
  margin-bottom: 1rem;
  text-shadow: 0 2px 20px rgba(0, 0, 0, 0.3);
  animation: fadeInUp 0.5s ease-out;
  transition: color 0.8s ease-in-out, text-shadow 0.8s ease-in-out;
}

.hero-subtitle {
  font-size: clamp(1.1rem, 2vw, 1.3rem);
  margin-bottom: 2rem;
  opacity: 0.95;
  font-weight: 400;
  text-shadow: 0 1px 10px rgba(0, 0, 0, 0.2);
  animation: fadeInUp 1s ease-out;
  transition: color 0.8s ease-in-out, text-shadow 0.8s ease-in-out;
}

.hero-slide {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  object-fit: cover;
  object-position: center;
  opacity: 0;
  transition: opacity 1.5s ease-in-out;
}

.hero-slide.active {
  opacity: 1;
}

/* CTA Buttons */
.cta-button {
  display: inline-block;
  padding: 1rem 2.5rem;
  font-size: 1.1rem;
  font-weight: 600;
  text-decoration: none;
  border-radius: 50px;
  transition: var(--transition);
  cursor: pointer;
  border: none;
  text-align: center;
  min-width: 200px;
}

.cta-button.primary {
  background: var(--aegean-blue);
  color: var(--white);
  box-shadow: 0 4px 20px rgba(30, 58, 138, 0.3);
}

.cta-button.primary:hover {
  background: var(--deep-blue);
  transform: translateY(-2px);
  box-shadow: 0 6px 25px rgba(30, 58, 138, 0.4);
}

.cta-button.secondary {
  background: var(--white);
  color: var(--aegean-blue);
  border: 2px solid var(--aegean-blue);
  box-shadow: 0 4px 20px rgba(30, 58, 138, 0.1);
}

.cta-button.secondary:hover {
  background: var(--aegean-blue);
  color: var(--white);
  transform: translateY(-2px);
  box-shadow: 0 6px 25px rgba(30, 58, 138, 0.3);
}

/* Main Content */
.main {
  padding-top: 4rem;
  width: 100vw;
  margin-left: calc(50% - 50vw);
}

.container {
  max-width: 1200px;
  width: 100%;
  margin: 0 auto;
  padding: 0 2rem;
  text-align: center; /* Center-align text */
}

.about-section {
  padding: 6rem 0;
  background: var(--cream); 
}

.about-content {
  text-align: center;
  max-width: 700px;
  margin: 0 auto;
  opacity: 0;
  transform: translateY(30px);
  transition: var(--transition);
}

.about-content.fade-in {
  opacity: 1;
  transform: translateY(0);
}

.about-content h2 {
  font-size: clamp(2rem, 4vw, 3rem);
  font-weight: 700;
  color: var(--aegean-blue);
  margin-bottom: 2rem;
}

.about-content p {
  font-size: 1.2rem;
  line-height: 1.8;
  color: var(--text-gray);
  margin-bottom: 3rem;
}

/* Footer */
.footer {
  background: var(--soft-blue);
  color: var(--text-dark);
  padding: 3rem 0 1rem;
}

.footer-content {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 2rem;
}

.footer-info h3 {
  font-size: 1.5rem;
  font-weight: 700;
  color: var(--aegean-blue);
  margin-bottom: 0.5rem;
}

.footer-info p {
  color: var(--text-gray);
  font-size: 0.95rem;
}

.footer-social {
  display: flex;
  gap: 1rem;
}

.footer-social a {
  color: var(--text-gray);
  transition: var(--transition);
  padding: 0.5rem;
  border-radius: 50%;
  background: rgba(30, 58, 138, 0.1);
}

.footer-social a:hover {
  color: var(--aegean-blue);
  background: rgba(30, 58, 138, 0.2);
  transform: translateY(-2px);
}

.footer-bottom {
  text-align: center;
  padding-top: 2rem;
  border-top: 1px solid rgba(30, 58, 138, 0.1);
}

.footer-bottom p {
  color: var(--text-gray);
  font-size: 0.9rem;
}

/* Animations */
@keyframes fadeInUp {
  from {
    opacity: 0;
    transform: translateY(30px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

/* Browse Restaurants Page */
.browse-restaurants {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 120px 0; /* Account for fixed header */
  min-height: 100vh;
  width: 100vw; /* Ensure it spans the full width */
  margin-left: calc(50% - 50vw); /* Center horizontally */
  background: var(--white);
}

.page-title {
  font-size: clamp(2.5rem, 4vw, 3.5rem);
  font-weight: 700;
  color: var(--aegean-blue);
  text-align: center;
  margin-bottom: 1rem;
}

.page-subtitle {
  font-size: 1.2rem;
  color: var(--text-gray);
  text-align: center;
  margin-bottom: 4rem;
}

.restaurants-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
  gap: 2rem;
  margin-top: 2rem;
}

.restaurant-filters {
  display: flex;
  gap: 1.1rem;
  margin-bottom: 2.5rem;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
  background: #f8fafc;
  border-radius: 32px;
  box-shadow: 0 2px 12px rgba(30, 58, 138, 0.06);
  padding: 1.1rem 2rem 1.1rem 2rem;
  max-width: 650px;
  margin-left: auto;
  margin-right: auto;
}
.restaurant-search, .restaurant-select {
  padding: 0.75rem 1.2rem;
  border: 1.5px solid #e0e7ef;
  border-radius: 24px;
  font-size: 1rem;
  outline: none;
  background: var(--white);
  min-width: 140px;
  transition: border 0.2s, box-shadow 0.2s;
  box-shadow: 0 1px 4px rgba(30, 58, 138, 0.04);
}
.restaurant-search {
  min-width: 200px;
}
.restaurant-search:focus, .restaurant-select:focus {
  border: 2px solid var(--aegean-blue);
  box-shadow: 0 2px 8px rgba(30, 58, 138, 0.10);
}
.restaurant-search:hover, .restaurant-select:hover {
  border: 1.5px solid var(--aegean-blue);
}

.no-restaurants {
  grid-column: 1/-1;
  text-align: center;
  color: var(--text-gray);
  font-size: 1.2rem;
  padding: 2rem 0;
}

.restaurant-card {
  background: var(--white);
  border-radius: 16px;
  box-shadow: 0 4px 20px var(--shadow);
  overflow: hidden;
  transition: transform 0.25s cubic-bezier(0.4,0,0.2,1), box-shadow 0.25s cubic-bezier(0.4,0,0.2,1);
  outline: none;
  cursor: pointer;
  display: flex;
  flex-direction: column;
  min-height: 420px;
  border: 2px solid transparent;
}
.restaurant-card:hover, .restaurant-card:focus {
  transform: translateY(-7px) scale(1.025);
  box-shadow: 0 8px 32px rgba(30, 58, 138, 0.18);
  border: 2px solid var(--aegean-blue);
}
.fade-in-card {
  opacity: 0;
  transform: translateY(30px);
  animation: fadeInUp 0.7s forwards;
}

.restaurant-image {
  width: 100%;
  height: 210px;
  overflow: hidden;
  background: var(--soft-blue);
  display: flex;
  align-items: center;
  justify-content: center;
}
.restaurant-image img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  transition: transform 0.5s cubic-bezier(0.4,0,0.2,1);
}
.restaurant-card:hover .restaurant-image img,
.restaurant-card:focus .restaurant-image img {
  transform: scale(1.04);
}
.restaurant-info {
  padding: 1.5rem 1.2rem 1.2rem 1.2rem;
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}
.restaurant-info h3 {
  font-size: 1.3rem;
  font-weight: 700;
  color: var(--aegean-blue);
  margin-bottom: 0.5rem;
}
.restaurant-cuisine-badge {
  display: inline-block;
  color: var(--white);
  font-size: 0.95rem;
  font-weight: 600;
  border-radius: 20px;
  padding: 0.3rem 1rem;
  margin-bottom: 0.7rem;
  margin-right: 0.5rem;
  background: var(--aegean-blue);
  letter-spacing: 0.5px;
}
.restaurant-description {
  color: var(--text-gray);
  line-height: 1.6;
  margin-bottom: 1.1rem;
  font-size: 1.05rem;
}
.restaurant-details {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  margin-bottom: 1.1rem;
  gap: 1.2rem;
}
.rating {
  color: #ffd700;
  font-weight: 600;
  display: flex;
  align-items: center;
  gap: 0.1rem;
  font-size: 1.1rem;
}
.star {
  color: #e0e0e0;
  font-size: 1.1rem;
  margin-right: 1px;
}
.star.filled {
  color: #ffd700;
}
.rating-value {
  color: var(--aegean-blue);
  font-size: 1rem;
  font-weight: 600;
  margin-left: 0.3rem;
}
.price-range {
  color: var(--aegean-blue);
  font-weight: 600;
  font-size: 1.05rem;
  letter-spacing: 0.5px;
  background: var(--soft-blue);
  border-radius: 12px;
  padding: 0.2rem 0.7rem;
}
.restaurant-book-btn {
  margin-top: auto;
  width: 100%;
  font-size: 1.08rem;
  font-weight: 700;
  letter-spacing: 0.5px;
  box-shadow: 0 2px 10px rgba(30, 58, 138, 0.08);
}
.restaurant-book-btn:focus {
  outline: 2px solid var(--aegean-blue);
  outline-offset: 2px;
}

@media (max-width: 900px) {
  .restaurants-grid {
    grid-template-columns: 1fr 1fr;
  }
  .restaurant-card {
    min-height: 440px;
  }
}
@media (max-width: 600px) {
  .restaurants-grid {
    grid-template-columns: 1fr;
  }
  .restaurant-card {
    min-height: 420px;
  }
  .restaurant-info {
    padding: 1.1rem 0.7rem 1rem 0.7rem;
  }
}

/* Responsive Design */
@media (max-width: 768px) {
  .nav-container {
    padding: 1rem;
  }

  .nav-links {
    position: fixed;
    top: 100%;
    left: 0;
    right: 0;
    background: rgba(255, 255, 255, 0.98);
    backdrop-filter: blur(10px);
    flex-direction: column;
    padding: 2rem;
    gap: 1.5rem;
    transform: translateY(-100%);
    opacity: 0;
    visibility: hidden;
    transition: var(--transition);
  }

  .nav-links.active {
    transform: translateY(0);
    opacity: 1;
    visibility: visible;
  }

  .mobile-menu-toggle {
    display: flex;
  }

  .mobile-menu-toggle.active span:nth-child(1) {
    transform: rotate(45deg) translate(5px, 5px);
  }

  .mobile-menu-toggle.active span:nth-child(2) {
    opacity: 0;
  }

  .mobile-menu-toggle.active span:nth-child(3) {
    transform: rotate(-45deg) translate(7px, -6px);
  }

  .hero-content {
    padding: 0 1rem;
  }

  .about-content p {
    font-size: 1.1rem;
  }

  .footer-content {
    flex-direction: column;
    gap: 2rem;
    text-align: center;
  }

  .cta-button {
    padding: 0.875rem 2rem;
    font-size: 1rem;
    min-width: 180px;
  }

  .restaurants-grid {
    grid-template-columns: 1fr;
    gap: 1.5rem;
  }
  
  .restaurant-card {
    margin: 0 1rem;
  }
}

@media (max-width: 480px) {
  .nav-container {
    padding: 0.75rem 1rem;
  }

  .container {
    padding: 0 1rem;
  }

  .about-section {
    padding: 4rem 0;
  }

  .hero-content {
    padding: 0 1rem;
  }

  .cta-button {
    padding: 0.75rem 1.5rem;
    font-size: 0.95rem;
    min-width: 160px;
  }
}

/* Accessibility */
@media (prefers-reduced-motion: reduce) {
  * {
    animation-duration: 0.01ms !important;
    animation-iteration-count: 1 !important;
    transition-duration: 0.01ms !important;
  }
}

/* Focus styles for accessibility */
.nav-links a:focus,
.cta-button:focus,
.footer-social a:focus {
  outline: 2px solid var(--aegean-blue);
  outline-offset: 2px;
}

/* High contrast mode support */
@media (prefers-contrast: high) {
  :root {
    --aegean-blue: #000080;
    --text-gray: #000000;
    --shadow: rgba(0, 0, 0, 0.3);
  }
}

/* --- HeroSection Enhancements --- */
.hero-wave {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  z-index: 2;
  pointer-events: none;
}

/* Fade-in for hero content */
.fade-in-hero {
  opacity: 1 !important;
  transform: translateY(0) scale(1) !important;
  transition: opacity 1.2s cubic-bezier(0.4,0,0.2,1), transform 1.2s cubic-bezier(0.4,0,0.2,1);
}
.hero-content {
  opacity: 0;
  transform: translateY(30px) scale(0.98);
  transition: opacity 1.2s cubic-bezier(0.4,0,0.2,1), transform 1.2s cubic-bezier(0.4,0,0.2,1);
}

/* Micro-interaction for CTA button */
.hero-cta {
  position: relative;
  overflow: hidden;
  outline: none;
}
.hero-cta:active {
  transform: scale(0.97);
}
.hero-cta::after {
  content: '';
  position: absolute;
  left: 50%;
  top: 50%;
  width: 0;
  height: 0;
  background: rgba(30, 58, 138, 0.15);
  border-radius: 100%;
  transform: translate(-50%, -50%);
  transition: width 0.4s cubic-bezier(0.4,0,0.2,1), height 0.4s cubic-bezier(0.4,0,0.2,1);
  pointer-events: none;
  z-index: 1;
}
.hero-cta:focus::after,
.hero-cta:hover::after {
  width: 250px;
  height: 250px;
}

@media (max-width: 768px) {
  .hero-wave {
    height: 40px;
  }
}

.hero-tagline {
  font-size: 1.25rem;
  font-weight: 500;
  color: var(--aegean-blue);
  margin-bottom: 1.2rem;
  letter-spacing: 0.5px;
  text-shadow: 0 1px 10px rgba(255,255,255,0.15);
  opacity: 0.92;
  animation: fadeInUp 1.2s 0.2s both;
}

/* Parallax effect for hero background */
.parallax-bg {
  perspective: 1px;
  overflow: hidden;
}
.parallax-bg img.hero-slide.active {
  will-change: transform;
  transition: transform 1.5s cubic-bezier(0.4,0,0.2,1), opacity 1.5s cubic-bezier(0.4,0,0.2,1);
}
@media (pointer: fine) {
  .parallax-bg img.hero-slide.active {
    transform: translateY(calc(var(--parallax, 0px)));
  }
}

.greek-divider {
  width: 100%;
  margin: 0 auto 2.5rem auto;
  display: flex;
  justify-content: center;
  align-items: center;
  background: none;
  height: 24px;
}

.how-it-works-section {
  background: var(--cream);
  padding: 4rem 0 3rem 0;
  text-align: center;
}
.how-title {
  font-size: 2rem;
  color: var(--aegean-blue);
  font-weight: 700;
  margin-bottom: 2.5rem;
  letter-spacing: 0.5px;
}
.how-steps {
  display: flex;
  justify-content: center;
  gap: 2.5rem;
  flex-wrap: wrap;
}
.how-step {
  background: var(--white);
  border-radius: 16px;
  box-shadow: 0 2px 12px var(--shadow);
  padding: 2rem 1.5rem 1.5rem 1.5rem;
  max-width: 260px;
  min-width: 200px;
  flex: 1 1 220px;
  display: flex;
  flex-direction: column;
  align-items: center;
  opacity: 0;
  transform: translateY(40px);
  transition: opacity 1s cubic-bezier(0.4,0,0.2,1), transform 1s cubic-bezier(0.4,0,0.2,1);
}
.how-step.visible {
  opacity: 1;
  transform: translateY(0);
}
.how-icon {
  margin-bottom: 1rem;
}
.how-step h4 {
  color: var(--aegean-blue);
  font-size: 1.15rem;
  font-weight: 700;
  margin-bottom: 0.5rem;
}
.how-step p {
  color: var(--text-gray);
  font-size: 1rem;
  margin-bottom: 0;
}

@media (max-width: 900px) {
  .how-steps {
    flex-direction: column;
    gap: 2rem;
  }
  .how-step {
    max-width: 100%;
    min-width: 0;
  }
}

.testimonial-section {
  background: var(--white);
  padding: 3.5rem 0 4rem 0;
  text-align: center;
}
.testimonial-content {
  max-width: 600px;
  margin: 0 auto;
  background: var(--cream);
  border-radius: 16px;
  box-shadow: 0 2px 12px var(--shadow);
  padding: 2.5rem 2rem 2rem 2rem;
  display: flex;
  flex-direction: column;
  align-items: center;
  opacity: 0;
  transform: translateY(40px);
  transition: opacity 1.2s cubic-bezier(0.4,0,0.2,1), transform 1.2s cubic-bezier(0.4,0,0.2,1);
}
.fade-in-testimonial.visible {
  opacity: 1;
  transform: translateY(0);
}
.testimonial-avatar {
  margin-bottom: 1.2rem;
}
.testimonial-content blockquote {
  font-size: 1.15rem;
  color: var(--aegean-blue);
  font-style: italic;
  margin-bottom: 1.2rem;
  line-height: 1.6;
  border-left: 4px solid var(--aegean-blue);
  padding-left: 1rem;
  background: rgba(224,242,254,0.25);
  border-radius: 8px;
}
.testimonial-author {
  color: var(--text-gray);
  font-size: 1rem;
  font-weight: 500;
}

.restaurant-image.compact {
  height: 140px;
  border-radius: 16px 16px 0 0;
}
.restaurant-info.compact {
  padding: 1.1rem 1rem 1rem 1rem;
}
.restaurant-header-row {
  display: flex;
  align-items: center;
