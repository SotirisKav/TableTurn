/* Live Operations Column */
.live-operations {
  background: white;
  border-radius: 12px;
  box-shadow: 0 4px 15px var(--shadow);
  overflow: hidden;
}

.last-updated {
  font-size: 0.8rem;
  color: var(--text-gray);
}

.table-status-grid {
  display: grid;
  grid-template-columns: 1fr 1fr;
  gap: 0.75rem;
  padding: 1.5rem;
}

.status-card {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 1rem;
  border-radius: 8px;
