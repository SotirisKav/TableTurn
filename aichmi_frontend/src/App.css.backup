/* Enhanced Weekly Outlook */
.weekly-outlook {
  background: #f8fafc;
  padding: 1.5rem 2rem;
  border-top: 1px solid #e2e8f0;
}

.weekly-header {
  display: flex;
  justify-content: space-between;
