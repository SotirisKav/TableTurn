/* Better message layout and spacing */
.message-with-map {
  width: 100%;
  max-width: 100%;
}

.message-text-before,
.message-text-after {
    margin-bottom: 1rem;
    line-height: 1.6;
}

.message-text-after {
    margin-top: 1rem;
    margin-bottom: 0;
}

.map-container {
    margin: 1.5rem 0;
    border-radius: 12px;
    overflow: hidden;
    box-shadow: 0 4px 16px rgba(30, 58, 138, 0.15);
    border: 2px solid var(--soft-blue);
    width: 100%;
    height: 350px; /* Fixed height for consistency */
}

/* Improve message content spacing */
.message-content {
    padding: 1.2rem 1.5rem;
    border-radius: 20px;
    position: relative;
    max-width: 100%;
    word-wrap: break-word;
    box-shadow: 0 2px 12px rgba(30, 58, 138, 0.08);
    line-height: 1.6;
}

/* Better scroll behavior */
.chat-messages {
    flex: 1;
    padding: 2rem;
    overflow-y: auto;
    overflow-x: hidden; /* Prevent horizontal scroll */
    display: flex;
    flex-direction: column;
    gap: 1.5rem;
    background: #fafbfc;
    scroll-behavior: smooth;
    max-height: none;
}

/* Ensure proper message width */
.message {
    display: flex;
    max-width: 90%; /* Increased from 85% */
    margin-bottom: 1rem;
    word-wrap: break-word;
}

.message.ai .message-content {
  max-width: 100%;
  width: 100%;
  background: var(--white);
  color: var(--text-dark);
  border-bottom-left-radius: 8px;
  border: 1px solid #e0e7ef;
  overflow: visible; /* Allow content to show */
}

.message.user .message-content {
    background: linear-gradient(135deg, var(--aegean-blue) 0%, var(--deep-blue) 100%);
    color: var(--white);
    border-bottom-right-radius: 8px;
    max-width: 100%;
}

/* Restaurant Registration Form - Integrated with App Design */

/* Step Indicator for Registration Form */
.auth-card .step-indicator {
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 20px 0;
    gap: 15px;
}

.auth-card .step {
    width: 45px;
    height: 45px;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #f8f9fa;
    color: #6c757d;
    font-weight: bold;
    border: 2px solid #e9ecef;
    transition: all 0.3s ease;
    font-size: 1rem;
}

.auth-card .step.active {
    background: var(--aegean-blue);
    border-color: var(--aegean-blue);
    color: white;
    box-shadow: 0 0 15px rgba(30, 58, 138, 0.3);
}

.auth-card .step.completed {
    background: #28a745;
    border-color: #28a745;
    color: white;
}

/* Step Content Headers */
.step-content h2 {
    color: var(--aegean-blue);
    margin-bottom: 8px;
    font-size: 1.8rem;
    text-align: center;
}

.step-content .auth-header {
    text-align: center;
    color: #6c757d;
    margin-bottom: 30px;
    font-size: 1.1rem;
}

/* Form Actions for Registration */
.form-actions {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 30px;
    gap: 20px;
}

.form-actions .cta-button {
    flex: 1;
    max-width: 200px;
    margin: 0;
}

.form-actions .cta-button.secondary {
    background: #f8f9fa;
    color: #6c757d;
    border: 2px solid #e9ecef;
}

.form-actions .cta-button.secondary:hover {
    background: #e9ecef;
    border-color: #adb5bd;
    color: #495057;
}

/* Pricing Options */
.pricing-options {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(180px, 1fr));
    gap: 15px;
    margin-top: 15px;
}

.pricing-option {
    display: flex;
    align-items: center;
    padding: 15px;
    border: 2px solid #e9ecef;
    border-radius: 8px;
    cursor: pointer;
    transition: all 0.3s ease;
    background: #f8f9fa;
}

.pricing-option:hover {
    border-color: var(--aegean-blue);
    background: #f0f4ff;
}

.pricing-option input[type="radio"] {
    margin-right: 12px;
    width: auto !important;
    transform: scale(1.2);
}

.pricing-option input[type="radio"]:checked + .pricing-label {
    color: var(--aegean-blue);
    font-weight: 600;
}

.pricing-label {
    font-size: 1rem;
    font-weight: 500;
}

/* Character Count */
.char-count {
    display: block;
    text-align: right;
    color: #6c757d;
    font-size: 0.85rem;
    margin-top: 5px;
    font-style: italic;
}

/* Selected Location Display */
.selected-location {
    margin-top: 12px;
    padding: 12px;
    background: #d4edda;
    border: 1px solid #c3e6cb;
    border-radius: 6px;
    color: #155724;
    font-weight: 500;
    font-size: 0.95rem;
}

/* Success Message for Registration */
.success-message {
    padding: 15px;
    background: #d4edda;
    border: 1px solid #c3e6cb;
    border-radius: 8px;
    color: #155724;
    text-align: center;
    font-weight: 600;
    margin-bottom: 20px;
    animation: fadeIn 0.5s ease-in;
}

@keyframes fadeIn {
    from { opacity: 0; transform: translateY(-10px); }
    to { opacity: 1; transform: translateY(0); }
}

/* Enhanced Form Row for Registration */
.auth-form .form-row {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 20px;
    margin-bottom: 20px;
}

/* Textarea Styling */
.auth-form textarea {
    min-height: 100px;
    resize: vertical;
    font-family: inherit;
    line-height: 1.5;
}

/* Error State for Form Fields */
.auth-form input.error,
.auth-form textarea.error {
    border-color: #dc3545;
    box-shadow: 0 0 0 2px rgba(220, 53, 69, 0.1);
}

/* Form Group Spacing */
.step-content .form-group {
    margin-bottom: 25px;
}

.step-content .form-group:last-child {
    margin-bottom: 0;
}

/* Mobile Responsive for Registration Form */
@media (max-width: 768px) {
    .auth-card .step-indicator {
        gap: 10px;
    }
    
    .auth-card .step {
        width: 40px;
        height: 40px;
        font-size: 0.9rem;
    }
    
    .auth-form .form-row {
        grid-template-columns: 1fr;
        gap: 15px;
    }
    
    .pricing-options {
        grid-template-columns: 1fr;
    }
    
    .form-actions {
        flex-direction: column;
        gap: 15px;
    }
    
    .form-actions .cta-button {
        max-width: none;
        width: 100%;
    }
    
    .step-content h2 {
        font-size: 1.5rem;
    }
}

@media (max-width: 480px) {
    .auth-card .step {
        width: 35px;
        height: 35px;
        font-size: 0.8rem;
    }
    
    .auth-card {
        margin: 10px;
        padding: 20px;
    }
}

/* Additional Registration Form Styles */

/* Manual Location Inputs */
.manual-location-inputs {
    background: #f8f9fa;
    padding: 20px;
    border-radius: 10px;
    border: 2px dashed #e9ecef;
}

.location-option-divider {
    text-align: center;
    margin: 20px 0;
    position: relative;
}

.location-option-divider span {
    background: white;
    padding: 0 15px;
    color: #6c757d;
    font-weight: 500;
}

.location-option-divider::before {
    content: '';
    position: absolute;
    top: 50%;
    left: 0;
    right: 0;
    height: 1px;
    background: #e9ecef;
    z-index: 0;
}

.change-location-btn {
    margin-left: 10px;
    background: none;
    border: 1px solid #6c757d;
    color: #6c757d;
    padding: 4px 12px;
    border-radius: 4px;
    font-size: 0.85rem;
