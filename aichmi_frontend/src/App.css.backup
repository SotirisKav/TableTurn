    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.08);
}

.day-checkbox {
    display: flex;
    align-items: center;
    gap: 12px;
    min-width: 120px;
}

.day-checkbox input[type="checkbox"] {
    width: 20px;
    height: 20px;
