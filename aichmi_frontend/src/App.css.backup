    accent-color: var(--aegean-blue);
    border-radius: 4px;
    margin: 0;
}

.day-checkbox label {
    font-weight: 500;
    color: #374151;
    cursor: pointer;
    margin: 0;
    font-size: 1rem;
}

.time-inputs {
    display: flex;
    align-items: center;
    gap: 12px;
    flex: 1;
}

.time-inputs input[type="time"] {
    flex: 1;
    min-width: 120px;
    padding: 12px 16px;
    border: 2px solid #e5e7eb;
    border-radius: 8px;
    font-size: 0.95rem;
    background: #fafafa;
    transition: all 0.3s ease;
    font-family: inherit;
    margin: 0;
}

.time-inputs input[type="time"]:focus {
    outline: none;
    border-color: var(--aegean-blue);
    background: white;
    box-shadow: 0 0 0 3px rgba(30, 58, 138, 0.1);
}

.time-inputs span {
    color: #64748b;
    font-weight: 500;
    white-space: nowrap;
    font-size: 0.9rem;
}

.day-row.disabled {
