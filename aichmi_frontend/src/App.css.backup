.kpi-label {
  font-size: 0.8rem;
  color: var(--text-gray);
  font-weight: 500;
  margin-bottom: 0.25rem;
}

.kpi-value {
  font-size: 1.4rem;
  font-weight: 700;
  color: var(--aegean-blue);
  margin-bottom: 0.25rem;
}

.kpi-change {
  font-size: 0.75rem;
  color: #10b981;
  font-weight: 500;
}

.kpi-target {
  font-size: 0.75rem;
  color: var(--text-gray);
}

.alerts-section {
  padding: 1.5rem;
  border-top: 1px solid #e2e8f0;
}

.alerts-section h4 {
  color: var(--aegean-blue);
  font-size: 1rem;
  margin-bottom: 1rem;
  font-weight: 600;
}

.alert {
  padding: 0.75rem;
  border-radius: 8px;
  border-left: 4px solid;
  margin-bottom: 0.75rem;
  transition: all 0.2s;
}

.alert:hover {
  transform: translateX(2px);
}

.alert-high {
  background: #fef2f2;
  border-left-color: #ef4444;
}

.alert-medium {
  background: #fefce8;
  border-left-color: #f59e0b;
}

.alert-low {
  background: #eff6ff;
  border-left-color: #3b82f6;
}

.alert-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 0.25rem;
}

.alert-title {
  font-weight: 600;
  font-size: 0.85rem;
  color: var(--aegean-blue);
}

.alert-time {
  font-size: 0.7rem;
  color: var(--text-gray);
}

.alert-message {
  font-size: 0.8rem;
  color: var(--text-gray);
  line-height: 1.4;
}

