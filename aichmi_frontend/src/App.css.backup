/* Single Plan Styling - Better and Smoother */
.single-plan-container {
  max-width: 480px;
  margin: 0 auto;
  perspective: 1000px;
}

.pricing-card.single-plan {
  background: linear-gradient(145deg, var(--white) 0%, #f8fafc 100%);
  border-radius: 24px;
  box-shadow: 
    0 8px 32px rgba(30, 58, 138, 0.12),
    0 2px 8px rgba(30, 58, 138, 0.08);
  padding: 3rem 2.5rem;
  text-align: center;
  position: relative;
  transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
  border: 3px solid transparent;
  background-clip: padding-box;
  transform: translateZ(0);
  overflow: hidden;
}

.pricing-card.single-plan::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: linear-gradient(145deg, 
    rgba(30, 58, 138, 0.03) 0%, 
    rgba(224, 242, 254, 0.8) 50%, 
    rgba(30, 58, 138, 0.03) 100%);
  opacity: 0;
  transition: opacity 0.4s cubic-bezier(0.4, 0, 0.2, 1);
  pointer-events: none;
  z-index: 1;
}

.pricing-card.single-plan:hover {
  transform: translateY(-8px) scale(1.02);
  box-shadow: 
    0 20px 60px rgba(30, 58, 138, 0.25),
    0 8px 24px rgba(30, 58, 138, 0.15);
  border-color: var(--aegean-blue);
}

.pricing-card.single-plan:hover::before {
  opacity: 1;
}

.pricing-card.single-plan .popular-badge {
  position: absolute;
  top: -16px;
  left: 50%;
  transform: translateX(-50%);
  background: linear-gradient(135deg, var(--aegean-blue) 0%, var(--deep-blue) 100%);
  color: var(--white);
  padding: 0.75rem 2rem;
  border-radius: 25px;
  font-size: 0.95rem;
  font-weight: 700;
  box-shadow: 0 4px 16px rgba(30, 58, 138, 0.4);
  letter-spacing: 0.5px;
  z-index: 3;
  position: relative;
}

.pricing-card.single-plan .plan-header {
  margin-bottom: 2.5rem;
  position: relative;
  z-index: 2;
}

.pricing-card.single-plan .plan-name {
  font-size: 1.8rem;
  font-weight: 800;
  color: var(--aegean-blue);
  margin-bottom: 1.5rem;
  letter-spacing: 0.5px;
}

.pricing-card.single-plan .plan-price {
  display: flex;
  align-items: baseline;
  justify-content: center;
  gap: 0.3rem;
  margin-bottom: 1.5rem;
}

.pricing-card.single-plan .price-currency {
  font-size: 1.5rem;
  color: var(--aegean-blue);
  font-weight: 700;
}

.pricing-card.single-plan .price-amount {
  font-size: 4rem;
  font-weight: 900;
  color: var(--aegean-blue);
  line-height: 0.9;
  text-shadow: 0 2px 4px rgba(30, 58, 138, 0.1);
}

.pricing-card.single-plan .price-period {
  font-size: 1.3rem;
  color: var(--text-gray);
  font-weight: 600;
}

.pricing-card.single-plan .plan-features {
  list-style: none;
  padding: 0;
  margin: 2.5rem 0;
  text-align: left;
  position: relative;
  z-index: 2;
}

.pricing-card.single-plan .feature-item {
  display: flex;
  align-items: center;
  gap: 1rem;
  padding: 1rem 0;
  color: var(--text-dark);
  font-weight: 600;
  font-size: 1.05rem;
  border-bottom: 1px solid rgba(224, 231, 239, 0.6);
  transition: all 0.3s ease;
}

.pricing-card.single-plan .feature-item:hover {
  padding-left: 0.5rem;
  color: var(--aegean-blue);
}

.pricing-card.single-plan .feature-item:last-child {
  border-bottom: none;
}

.pricing-card.single-plan .feature-check {
  color: #22c55e;
  font-weight: 800;
  font-size: 1.3rem;
  flex-shrink: 0;
  background: rgba(34, 197, 94, 0.1);
  border-radius: 50%;
  width: 24px;
  height: 24px;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 1rem;
}

.pricing-card.single-plan .plan-button {
  width: 100%;
  padding: 1.2rem 2rem;
  font-size: 1.2rem;
  font-weight: 800;
  border-radius: 16px;
  margin-top: 1.5rem;
  background: linear-gradient(135deg, var(--aegean-blue) 0%, var(--deep-blue) 100%);
  box-shadow: 0 6px 20px rgba(30, 58, 138, 0.3);
  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
  letter-spacing: 0.5px;
  text-transform: uppercase;
  position: relative;
  z-index: 2;
  overflow: hidden;
}

.pricing-card.single-plan .plan-button::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, 
    transparent, 
    rgba(255, 255, 255, 0.3), 
    transparent);
  transition: left 0.6s ease;
}

.pricing-card.single-plan .plan-button:hover {
  transform: translateY(-2px);
  box-shadow: 0 10px 30px rgba(30, 58, 138, 0.4);
}

.pricing-card.single-plan .plan-button:hover::before {
  left: 100%;
}

.pricing-card.single-plan .plan-button:active {
  transform: translateY(0);
  box-shadow: 0 4px 15px rgba(30, 58, 138, 0.3);
}

