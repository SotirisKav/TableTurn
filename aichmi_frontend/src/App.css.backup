  font-size: 2rem;
  font-weight: 800;
  color: var(--aegean-blue);
  line-height: 1;
  margin-bottom: 0.5rem;
}

.modal-stat .stat-label {
  font-size: 0.8rem;
  color: var(--text-gray);
  font-weight: 600;
  text-transform: uppercase;
  letter-spacing: 0.05em;
}

.modal-reservations {
  padding: 1.5rem 2rem;
  display: flex;
  flex-direction: column;
  gap: 1.5rem;
  max-height: 50vh;
  overflow-y: auto;
}

.reservation-detail-card {
  background: #f8fafc;
  border: 1px solid #e2e8f0;
  border-radius: 12px;
  padding: 1.5rem;
  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
}

.reservation-detail-card:hover {
  background: white;
  border-color: var(--aegean-blue);
  box-shadow: 0 4px 12px rgba(30, 58, 138, 0.1);
  transform: translateY(-1px);
}

.reservation-header {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  margin-bottom: 1rem;
  padding-bottom: 1rem;
  border-bottom: 1px solid #e2e8f0;
}

.guest-info h4 {
  font-size: 1.2rem;
  font-weight: 700;
  color: var(--text-dark);
  margin: 0 0 0.5rem 0;
}

.contact-info {
  font-size: 0.85rem;
  color: var(--text-gray);
  margin: 0;
}

.reservation-time {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  gap: 0.5rem;
}

.reservation-time .time {
  font-size: 1.1rem;
  font-weight: 700;
  color: var(--aegean-blue);
}

.reservation-details {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
  gap: 0.75rem;
}

.detail-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0.5rem 0;
}

.detail-label {
  font-size: 0.85rem;
  color: var(--text-gray);
  font-weight: 600;
  display: flex;
  align-items: center;
  gap: 0.5rem;
}

.detail-value {
  font-size: 0.9rem;
  color: var(--text-dark);
  font-weight: 600;
  text-align: right;
}

.detail-value.celebration {
  color: #f59e0b;
  font-weight: 700;
}

/* View Details Button */
.view-details-btn {
  background: linear-gradient(135deg, var(--aegean-blue) 0%, var(--deep-blue) 100%);
  color: white;
  border: none;
  padding: 0.75rem 1.5rem;
  border-radius: 20px;
  font-size: 0.85rem;
  font-weight: 600;
  cursor: pointer;
  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
  display: flex;
  align-items: center;
  gap: 0.5rem;
}

.view-details-btn:hover {
  transform: translateY(-2px);
  box-shadow: 0 6px 16px rgba(30, 58, 138, 0.3);
}

/* Clickable day bars */
.day-bar.clickable {
  cursor: pointer;
  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
}

.day-bar.clickable:hover {
  transform: translateY(-2px);
}

.day-bar.clickable:hover .bar {
  box-shadow: 0 6px 16px rgba(30, 58, 138, 0.2);
}

/* Mobile Responsiveness for Modal */
@media (max-width: 768px) {
  .modal-content {
    width: 95vw;
    max-height: 95vh;
  }
  
  .modal-header {
    padding: 1rem 1.5rem;
  }
  
  .modal-header h2 {
    font-size: 1.2rem;
  }
  
  .modal-summary {
    padding: 1rem 1.5rem;
    gap: 2rem;
  }
  
  .modal-reservations {
    padding: 1rem 1.5rem;
  }
  
  .reservation-header {
    flex-direction: column;
    gap: 1rem;
    align-items: flex-start;
  }
  
  .reservation-time {
    align-items: flex-start;
  }
  
  .reservation-details {
    grid-template-columns: 1fr;
  }
  
  .detail-item {
    flex-direction: column;
    align-items: flex-start;
    gap: 0.25rem;
  }
  
  .detail-value {
    text-align: left;
  }
}

/* Admin navigation styling */
.nav-links .admin-nav {
  background: linear-gradient(135deg, #ef4444 0%, #dc2626 100%) !important;
  color: white !important;
  padding: 0.5rem 1rem !important;
  border-radius: 6px !important;
  font-weight: 600 !important;
  transition: var(--transition) !important;
}
.nav-links .admin-nav:hover {
  background: linear-gradient(135deg, #dc2626 0%, #b91c1c 100%) !important;
  color: white !important;
  transform: translateY(-1px) !important;
}
.nav-links .admin-nav::after {
  display: none !important;
}

