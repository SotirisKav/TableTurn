/* Tier 1 Dashboard */
.snapshot-cards {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
  gap: 1.5rem;
  margin-bottom: 2rem;
}

.snapshot-card {
  background: white;
  padding: 2rem;
  border-radius: 16px;
  box-shadow: 0 6px 20px rgba(30, 58, 138, 0.1);
  text-align: center;
  border-top: 4px solid var(--aegean-blue);
  border: none;
  transition: var(--transition);
}

.snapshot-card h3 {
  color: var(--text-gray);
  font-size: 0.9rem;
  margin-bottom: 0.5rem;
  font-weight: 500;
}

.snapshot-number {
  font-size: 3.5rem;
  font-weight: 800;
  color: var(--aegean-blue);
  margin: 1rem 0;
  text-shadow: 0 2px 4px rgba(30, 58, 138, 0.1);
}

.weekly-demand {
  background: white;
  padding: 1.5rem;
  border-radius: 12px;
  box-shadow: 0 4px 15px var(--shadow);
  margin-bottom: 2rem;
}

.demand-chart {
  display: flex;
  gap: 1rem;
  justify-content: space-around;
  align-items: end;
  height: 200px;
  margin-top: 1rem;
}

.demand-bar {
  display: flex;
  flex-direction: column;
  align-items: center;
  min-width: 60px;
}

.bar {
  background: linear-gradient(135deg, var(--aegean-blue), var(--deep-blue));
  border-radius: 4px 4px 0 0;
  width: 40px;
  display: flex;
  align-items: end;
  justify-content: center;
  color: white;
  font-weight: 600;
  font-size: 0.8rem;
  padding-bottom: 0.25rem;
}

.bar-label {
  font-size: 0.7rem;
  color: var(--text-gray);
  margin-top: 0.5rem;
  text-align: center;
}

.alerts-section {
  background: white;
  padding: 1.5rem;
  border-radius: 12px;
  box-shadow: 0 4px 15px var(--shadow);
}

.alerts-list {
  display: flex;
  flex-direction: column;
  gap: 0.75rem;
  margin-top: 1rem;
}

.alert {
  padding: 1rem;
  border-radius: 8px;
  border-left: 4px solid;
}

.alert-large_party {
  background: var(--soft-blue);
  border-left-color: var(--aegean-blue);
}

.alert-celebration {
  background: #fef3c7;
  border-left-color: #f59e0b;
}

.alert-message {
  font-weight: 500;
  margin-bottom: 0.25rem;
}

.alert-time {
  font-size: 0.8rem;
  color: var(--text-gray);
}

.no-alerts {
  text-align: center;
  color: var(--text-gray);
  font-style: italic;
  padding: 2rem;
}

/* Tier 2 Analytics */
.analytics-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
  gap: 1.5rem;
}

.analytics-card, .insights-card, .performance-card {
  background: white;
  padding: 1.5rem;
  border-radius: 12px;
  box-shadow: 0 4px 15px var(--shadow);
  border: 1px solid var(--soft-blue);
}

.analytics-card h3, .insights-card h3, .performance-card h3 {
  color: var(--aegean-blue);
  margin-bottom: 1rem;
  font-weight: 600;
}

.trend-item, .lead-time-item, .celebration-item, .hotel-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0.5rem 0;
  border-bottom: 1px solid var(--soft-blue);
}

.trend-item:last-child, .lead-time-item:last-child, 
.celebration-item:last-child, .hotel-item:last-child {
  border-bottom: none;
}

.revenue-stats {
  display: flex;
  flex-direction: column;
  gap: 1rem;
}

.revenue-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0.75rem;
  background: var(--soft-blue);
  border-radius: 8px;
}

/* Tier 3 & 4 Styles */
.insights-grid, .ai-performance-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
  gap: 1.5rem;
}

.no-data {
  text-align: center;
  color: var(--text-gray);
  font-style: italic;
  padding: 1rem;
}

.interaction-number {
  font-size: 2rem;
  font-weight: 700;
  color: var(--aegean-blue);
  text-align: center;
}

.interaction-message, .semantic-info p, .dietary-info p, .agent-info p, .missed-info p {
  color: var(--text-gray);
  font-style: italic;
  text-align: center;
  margin-top: 0.5rem;
}

/* Error and Loading States */
.dashboard-error {
  text-align: center;
  padding: 4rem 2rem;
}

.dashboard-error h2 {
  color: var(--aegean-blue);
  margin-bottom: 1rem;
}

.dashboard-error button {
  background: var(--aegean-blue);
  color: white;
  border: none;
  padding: 0.75rem 1.5rem;
  border-radius: 8px;
  cursor: pointer;
  margin-top: 1rem;
  transition: var(--transition);
}

.dashboard-error button:hover {
  background: var(--deep-blue);
}

.dashboard-loading {
  text-align: center;
  padding: 4rem 2rem;
  color: var(--text-gray);
}

/* Responsive Design */
@media (max-width: 768px) {
  .dashboard-container {
    padding: 5rem 1rem 2rem;
  }
  
  .dashboard-header {
    flex-direction: column;
    gap: 1rem;
    text-align: center;
  }
  
  .dashboard-tabs {
    flex-wrap: wrap;
    justify-content: center;
  }
  
  .snapshot-cards {
    grid-template-columns: 1fr;
  }
  
  .demand-chart {
    overflow-x: auto;
    padding-bottom: 1rem;
  }
  
  .analytics-grid, .insights-grid, .ai-performance-grid {
    grid-template-columns: 1fr;
  }
}

/* Enhanced Dashboard Layout Styles */
.live-status-header {
